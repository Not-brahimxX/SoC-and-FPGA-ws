<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="prj_servo_btns"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
prj_servo_btns
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="prj_servo_btns">
        <sdx:configuration sdx:name="prj_servo_btns"
                           sdx:displayName="prj_servo_btns"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>prj_servo_btns</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="prj_servo_btns/boot/prj_servo_btns.bif"
                            sdx:readme="prj_servo_btns/boot/generic.readme"
                            sdx:qemuArguments="prj_servo_btns/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="prj_servo_btns/boot/prj_servo_btns.bif"
                            sdx:readme="prj_servo_btns/boot/generic.readme"
                            sdx:qemuArguments="prj_servo_btns/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="prj_servo_btns/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="prj_servo_btns/standalone_domain/bspinclude/include"
                        sdx:bspConfig="prj_servo_btns/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
